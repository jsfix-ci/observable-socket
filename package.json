{
  "name": "observable-socket",
  "version": "5.0.2",
  "description": "Wraps a websocket in an Observable.",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "build": "./node_modules/.bin/cross-env NODE_ENV=production node build.js",
    "prepublish": "npm run build",
    "pretest": "npm run build",
    "test": "standard && mocha __test"
  },
  "standard": {
    "globals": [
      "WebSocket",
      "afterEach",
      "beforeEach",
      "describe",
      "it",
      "xit"
    ]
  },
  "keywords": [
    "observable",
    "rx",
    "rxjs",
    "socket",
    "websocket",
    "ws"
  ],
  "bugs": {
    "url": "https://github.com/killtheliterate/observable-socket/issues"
  },
  "contributors": [
    "Garrett Dawson <killtheliterate@gmail.com> (http://thedrearlight.com/)",
    "Talon Poole <talon@theghostin.me> (http://theghostin.me)"
  ],
  "homepage": "https://github.com/killtheliterate/observable-socket",
  "repository": {
    "type": "git",
    "url": "https://github.com/killtheliterate/observable-socket.git"
  },
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "rxjs": "^5.4.1"
  },
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "chai": "^3.4.1",
    "cross-env": "^5.0.1",
    "mocha": "^2.3.4",
    "mock-socket": "^6.0.4",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0",
    "standard": "^8.6.0",
    "ws": "^0.8.1"
