    "debug": "^2.2.0",
    "lodash": "^3.10.1",
    "rx": "^4.0.7"
  },
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-core": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "karma": "^0.13.16",
    "karma-browserify": "^4.4.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-mocha": "^0.2.1",
    "karma-spec-reporter": "0.0.23",
    "mocha": "^2.3.4",
    "ws": "^0.8.1"
