{
  "name": "observable-socket",
  "version": "2.0.2",
  "description": "Wraps a websocket in an Observable.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist && babel src --out-dir dist",
    "prepublish": "rm -rf ./dist && babel src --out-dir dist",
    "pretest": "rm -rf ./dist && babel src --out-dir dist",
    "test:node": "npm run pretest; ./node_modules/.bin/mocha test/node",
    "test:browser": "npm run pretest; ./node_modules/.bin/karma start karma.conf.js",
    "test": "./node_modules/.bin/mocha test/node; ./node_modules/.bin/karma start karma.conf.js"
  },
  "keywords": [
    "socket",
    "observable",
    "ws",
    "websocket"
  ],
  "bugs": {
    "url": "https://github.com/killtheliterate/observable-socket/issues"
  },
  "contributors": [
    "Garrett Dawson <killtheliterate@gmail.com> (http://thedrearlight.com/)",
    "Talon Poole <talon@theghostin.me> (http://theghostin.me)"
  ],
  "homepage": "https://github.com/killtheliterate/observable-socket",
  "repository": {
    "type": "git",
    "url": "https://github.com/killtheliterate/observable-socket.git"
  },
  "license": "MIT",
